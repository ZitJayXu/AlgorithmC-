#include<iostream>
#include<vector>
using namespace std;
int Binaryfind(vector<int>& arr, int key) {
	int left = 0, right = arr.size(),mid;
	while (left < right) {
		mid = (left + right) >> 1;
		if (arr[mid] > key) {
			right = mid;
		}
		else if (arr[mid] < key) {
			left = mid + 1;
		}
		else {
			return mid;
		}
	}
	return -1;
}
int main() {
	vector<int>s{1,2,3,4,12,34,555,1234,5321};
	cout << Binaryfind(s, 1) << endl;
	return 0;
}
